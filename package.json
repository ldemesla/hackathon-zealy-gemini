{
  "name": "hackathon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --experimental-https --experimental-https-cert=./certificates/localhost.cert --experimental-https-key=./certificates/localhost.key",
    "create-cert": "HOMEBREW_NO_AUTO_UPDATE=1 bash src/scripts/create-cert.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:infrastructure": "tsc src/infrastructure/database/migrations/*.ts --outDir ./src/infrastructure/dist --target ES2015 --module CommonJS --esModuleInterop true",
    "config:check": "tsx src/infrastructure/config/server.config.ts",
    "db:migrate:down": "pnpm kysely migrate:down",
    "db:migrate:latest": "pnpm kysely migrate:latest && pnpm db:typegen && pnpm build:infrastructure",
    "db:migrate:latest:all": "pnpm db:migrate:latest && pnpm db:migrate:latest:test",
    "db:migrate:latest:test": "NODE_ENV=test pnpm kysely migrate:latest",
    "db:migrate:make": "pnpm kysely migrate:make",
    "db:migrate:up": "pnpm kysely migrate:up && pnpm db:typegen",
    "db:setup": "docker compose up -d",
    "db:typegen": "pnpm kysely-codegen --camel-case --dialect postgres --out-file ./src/infrastructure/database/__generated__/index.ts",
    "lint:fix": "prettier . --ignore-path ../../.prettierignore --write --log-level warn"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@types/pg": "^8.11.11",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.4.2",
    "inngest": "^3.31.7",
    "ioredis": "^5.5.0",
    "kysely": "^0.27.5",
    "kysely-codegen": "^0.17.0",
    "kysely-ctl": "^0.11.0",
    "lucide-react": "^0.475.0",
    "next": "15.1.7",
    "pg": "^8.13.2",
    "postgres-array": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
